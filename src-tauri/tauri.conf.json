{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "passenger-ui",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "sidecar": true,
        "execute": true,
        "all": false,
        "open": true,
        "scope": [
          {
            "cmd": "sh",
            "args": true,
            "name": "sh",
            "sidecar": false
          }
        ]
      }
    },
    "windows": [
      {
        "title": "passenger-ui",
        "width": 800,
        "height": 600,
        "minHeight": 392,
        "minWidth": 392,
        "acceptFirstMouse": true,
        "contentProtected": true,
        "fileDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "dev.elagoht.passenger",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}